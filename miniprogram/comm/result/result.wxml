<view class="page__hd">
     <button class="weui-btn" type="primary" bindtap='back'>返回</button>
</view>
<view class="page__bd">
    <view class="weui-form-preview">
      <view class="weui-form-preview__hd">
          <view class="weui-form-preview__item">
              <view class="weui-form-preview__label">平均加班时长</view>
              <view class="weui-form-preview__value_in-hd">{{avgDate}}</view>
          </view>
      </view>
    </view>
    <view class="weui-form-preview"  wx:for="{{searchData}}" data-item="item" wx:key="_id" >
        <view class="weui-form-preview__hd">
            <view class="weui-form-preview__item">
                <view class="weui-form-preview__label">打卡日期</view>
                <view class="weui-form-preview__value_in-hd">{{item.date}}</view>
            </view>
        </view>
        <view class="weui-form-preview__bd">
            <view class="weui-form-preview__item">
                <view class="weui-form-preview__label">打卡时间</view>
                <view class="weui-form-preview__value">{{item.time}}</view>
            </view>
            <view class="weui-form-preview__item">
                <view class="weui-form-preview__label">是否通宵</view>
                <view class="weui-form-preview__value">{{item.overTime}}</view>
            </view>
        </view>
        <view class="weui-form-preview__ft">
            <view url="" bindtap='operate' data-item='{{item}}' class="weui-form-preview__btn weui-form-preview__btn_primary" hover-class="weui-form-preview__btn_active">操作</view>
            <modal title="标题" confirm-text="确认" cancel-text="取消" hidden="onOff" bindconfirm="modalConfirm" bindcancel="modalCancel">这里是会话内容
</modal>
        </view>
    </view>
     
</view>